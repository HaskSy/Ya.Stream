{
    "name": "Ya.Stream",
    "description": "Listen music together",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {},
    "action": {
        "default_popup":"./popup.html"
    },
    "permissions": [
        "storage",
        "cookies"
    ],
    "background":{
        "service_worker": "./service-worker.js",
        "type": "module"
    },
    "host_permissions": [
        "https://music.gvsem.com/"
    ],
    "web_accessible_resources": [{
          "resources": [ "finalredirect.html" ],
          "matches": [ "https://music.gvsem.com/*" ]
    }]
}
