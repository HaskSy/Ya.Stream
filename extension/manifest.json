{
    "name": "Ya.Stream",
    "description": "Listen music together",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {},
    "action": {
        "default_popup":"popup.html"
    },
    "permissions": [
        "storage"
    ],
    "background":{
        "service_worker": "service-worker.js",
        "type": "module"
    },
    "web_accessible_resources": [{
        "resources": ["popup.html"],
        "matches": ["https://music.gvsem.com/*"],
        "use_dynamic_url": true
    }]
}
